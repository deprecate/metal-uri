{
  "name": "metal-uri",
  "version": "3.1.0",
  "description": "Class for parsing and formatting URIs.",
  "license": "BSD",
  "repository": "metal/metal-uri",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "lib/Uri.js",
  "module": "lib/Uri.js",
  "jsnext:main": "src/Uri.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "checkFormat": "npm run prettier -- --list-different",
    "compile": "babel --presets env -d lib/ src/",
    "format": "npm run prettier -- --write",
    "lint": "eslint '{src,test}/**/*.js'",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint '{src,test}/**/*.js'",
    "prepublish": "npm run compile",
    "test": "karma start",
    "test:saucelabs": "karma start karma-saucelabs.conf.js"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal": "^2.0.0",
    "metal-structs": "^1.0.0",
    "path-browserify": "0.0.0",
    "resolve-pathname": "^2.2.0",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.10.0",
    "eslint-config-liferay": "^2.0.9",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "~1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.5",
    "lint-staged": "^4.3.0",
    "mocha": "^4.0.1",
    "prettier-eslint-cli": "^4.4.0",
    "sinon": "^4.1.2",
    "webpack": "^3.5.5"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "react-native": {
    "path": "path-browserify"
  }
}
